<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="ClinicDomainModel" transaction-type="RESOURCE_LOCAL">
		<class>edu.stevens.cs.cs548.clinic.domain.Patient</class>
		<class>edu.stevens.cs.cs548.clinic.domain.Clinic</class>
		<class>edu.stevens.cs.cs548.clinic.domain.Treatment</class>
		<class>edu.stevens.cs.cs548.clinic.domain.DrugTreatment</class>
		<class>edu.stevens.cs.cs548.clinic.domain.Provider</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/sample;create=true"/>
			<property name="javax.persistence.jdbc.user" value="user"/>
			<property name="javax.persistence.jdbc.password" value="123"/>
			<property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
		</properties>
	</persistence-unit>
</persistence>
